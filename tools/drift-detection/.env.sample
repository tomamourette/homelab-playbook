# SSH Configuration
# Path to SSH private key (leave empty to use default SSH agent)
SSH_KEY_PATH=~/.ssh/id_rsa

# Target Hosts (comma-separated)
TARGET_HOSTS=192.168.50.19,192.168.50.161

# SSH Username
SSH_USERNAME=root

# SSH Connection Timeout (seconds)
SSH_TIMEOUT=10

# Docker Connection Timeout (seconds)
DOCKER_TIMEOUT=30

# Output Settings
OUTPUT_FORMAT=json
OUTPUT_DIR=./output

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
